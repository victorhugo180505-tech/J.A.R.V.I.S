{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jarvis_avatar_tauri",
  "version": "0.1.0",
  "identifier": "com.victo.jarvis_avatar_tauri",
 "build": {
  "devUrl": "http://localhost:5173",
  "frontendDist": "../dist",
  "beforeDevCommand": "npm run dev",
  "beforeBuildCommand": "npm run build"
},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "JARVIS_AVATAR__5f3c9e",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
